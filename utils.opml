				<outline text="else " created="Sat, 09 Feb 2013 17:35:33 GMT" pgfnum="18449">
					<outline text="s = hour + &quot;:&quot; + minutes + ampm" created="Sat, 09 Feb 2013 17:35:47 GMT" pgfnum="18451"/>
