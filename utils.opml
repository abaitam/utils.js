				<outline text="if (hour &gt;= 12) " created="Tue, 15 Jan 2013 14:31:54 GMT" pgfnum="17144">
					<outline text="ampm = &quot;PM&quot;" created="Tue, 15 Jan 2013 14:32:11 GMT" pgfnum="17146"/>
